@using VendorTracker.Models;

@if (@Model.Count == 0)
{
  <h3>No categories have been added yet!</h3>
} 
<ol>
@foreach(Vendor vendor in Model)
{
  <li><h3><a href='/vendors/@vendor.Id'>@vendor.Name</a></h3></li>
}
</ol>
 
<p><a href='/vendors/new'>Add a Vendor</a></p>
<p><a href='/'>Back home</a></p>